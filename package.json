{
  "name": "hook-easy-form",
  "version": "1.4.1",
  "main": "build/index.umd.js",
  "types": "build/declaration/index.d.ts",
  "homepage": "https://github.com/hook-easy-form/hook-easy-form.git#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hook-easy-form/hook-easy-form.git"
  },
  "files": [
    "build"
  ],
  "scripts": {
		"test:clear": "jest --clearCache",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "build": "npm run clean && tsc -p tsconfig.json && rollup -c rollup.config.js",
    "postbuild": "cp -R declaration build/",
    "clean": "rimraf dist/* && rimraf build/*",
    "dev": "nodemon --watch src -e ts, --exec npm run dev:start",
    "dev:start": "npm run build && npm run start",
    "start": "node .",
    "prettier:ts": "prettier --write './src/**/*.ts' --config ./.prettierrc",
    "prettier:tsx": "prettier --write './src/**/*.tsx' --config ./.prettierrc"
  },
  "keywords": [
    "react",
    "hooks",
    "typescript",
    "form"
  ],
  "author": "EntonyStark",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.3",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "husky": "^4.2.3",
    "jest": "^25.2.1",
    "lint-staged": "^10.0.9",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "typescript-tslint-plugin": "^0.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && lint-staged"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run prettier:ts"
    ],
    "./src/**/*.tsx": [
      "npm run prettier:tsx"
    ]
  },
  "dependencies": {},
  "description": "Easy manager for forms in react"
}
